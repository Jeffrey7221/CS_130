# Multiple statements - Expect Success

server {
  root { 
    http 192.23.134.3;
    listen 1024;
    listen 1023;
  }
}

server {
  root { 
    http 192.23.134.3;
    listen 0;
  }

  path {
    # another comment
    foo "bar";
    tcp {
      # another nested comment
      bar "foo";
    }
  }
}

