# Format of config file we use for our webserver

server {
  port 80;
  server_name foo.com;

  location "/static" StaticHandler {
    root /data;
  }

  location "/status" StatusHandler {
  }

  location "/echo" EchoHandler {
  }

  location "/" BadRequestHandler {
  }

  location "/ucla" ReverseProxyHandler {
    proxy_port 80;
    proxy_dest www.ucla.edu;
  }

  # Call /local/redirect 
  location "/local" ReverseProxyHandler {
    proxy_port 80;
    proxy_dest 35.185.213.80;
  }

  location "/redirect" RedirectHandler {
    port 80
    host 35.185.213.80;
  }
}